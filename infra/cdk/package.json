{
  "name": "bidopsai-cdk",
  "version": "1.0.0",
  "description": "AWS CDK Infrastructure for BidOps.AI",
  "main": "bin/bidopsai.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:cognito": "cdk deploy BidOpsAI-Cognito-dev",
    "deploy:s3": "cdk deploy BidOpsAI-S3SourceBucket-dev",
    "deploy:dev": "cdk deploy --all --context environment=dev",
    "deploy:staging": "cdk deploy --all --context environment=staging",
    "deploy:prod": "cdk deploy --all --context environment=prod",
    "destroy": "cdk destroy --all",
    "destroy:cognito": "cdk destroy BidOpsAI-Cognito-dev",
    "destroy:s3": "cdk destroy BidOpsAI-S3SourceBucket-dev",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "outputs": "cdk deploy --all --outputs-file outputs.json"
  },
  "keywords": [
    "aws-cdk",
    "cognito",
    "s3",
    "infrastructure"
  ],
  "author": "BidOps.AI Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "aws-cdk": "^2.219.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.219.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}