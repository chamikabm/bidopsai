.pytest_cache/
.coverage
coverage.xml
htmlcov/
.tox/
.hypothesis/

# Type checking
.mypy_cache/
.dmypy.json
dmypy.json
.pytype/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

