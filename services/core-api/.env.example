# Application
NODE_ENV=development
PORT=4000

# Database (matches docker-compose.dev.yml)
DATABASE_URL=postgresql://bidopsai:bidopsai_dev_password@localhost:5432/bidopsai?schema=public

# AWS Configuration
AWS_REGION=ap-southeast-2
AWS_ACCESS_KEY_ID=your-access-key-here
AWS_SECRET_ACCESS_KEY=your-secret-key-here
AWS_COGNITO_USER_POOL_ID=your-cognito-pool-id
AWS_COGNITO_CLIENT_ID=your-cognito-client-id
S3_BUCKET_NAME=bidopsai-documents-dev
JWT_ISSUER=https://cognito-idp.ap-southeast-2.amazonaws.com/your-cognito-pool-id

# Logging
LOG_LEVEL=debug

# Redis (Optional - for production subscriptions)
# REDIS_URL=redis://redis:6379

# CORS
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100